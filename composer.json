{
    "name": "toyokumo/kintone-query-builder",
    "description": "kintone query builder",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {"KintoneQueryBuilder\\" : "src/"}
    },
    "scripts": {
        "test": "phpunit",
        "coverage": "phpunit --coverage-text --coverage-html tests/_output",
        "format": "prettier --write src/*.php tests/*.php"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    }
}
